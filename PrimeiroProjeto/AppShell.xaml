<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PrimeiroProjeto.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PrimeiroProjeto"
    Shell.FlyoutBehavior="Flyout"
    Title="PrimeiroProjeto">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent 
        Title="Questoes"
        ContentTemplate="{DataTemplate local:Questoes}"
        Route="Questoes" />

    <ShellContent 
       Title="Positivo"
       ContentTemplate="{DataTemplate local:Positivo}"
       Route="Positivo" />

    <ShellContent 
       Title="Negativo"
       ContentTemplate="{DataTemplate local:Negativo}"
       Route="Negativo" />

</Shell>
